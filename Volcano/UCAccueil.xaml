<UserControl x:Class="Volcano.UCAccueil"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Volcano"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/image/image page accueil.png"/>
        </Grid.Background>
        <Button x:Name="butjouer" Content="Jouer" Canvas.Left="299" Canvas.Top="256" Height="50" Width="202" Background="#FFFFAD21" HorizontalAlignment="Center" VerticalAlignment="Top" Foreground="White" Margin="0,256,0,0">
            <Button.Effect>
                <DropShadowEffect/>
            </Button.Effect>
        </Button>
        <Button x:Name="but_parametre" Content="Paramètre" Canvas.Left="299" Canvas.Top="335" Height="50" Width="202" HorizontalAlignment="Center" VerticalAlignment="Top" Background="#FFFFAD21" Foreground="White" Margin="0,344,0,0">
            <Button.Effect>
                <DropShadowEffect/>
            </Button.Effect>
        </Button>
    </Grid>
</UserControl>
